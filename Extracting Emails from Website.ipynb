    "email_format = r\"[\\w\\.-]+@[\\w\\.-]+\"\n",
    "email_list = list()\n",
    "\n",
    "for email in re.findall(email_format, request.text):\n",
    "    email_list.append(email)\n",
    "print(email_list)"
